{
  "name": "stack-stacky",
  "version": "1.0.0",
  "description": "Stackoverflow clone",
  "main": "server.js",
  "repository": "https://github.com/eokwukwe/stack-stacky.git",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/server.js",
    "start": "node ./dist/index.js",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "test": "cross-env NODE_ENV=test jest --no-cache  --detectOpenHandles --runInBand --coverage --colors --forceExit"
  },
  "keywords": [
    "node",
    "express",
    "mongodb"
  ],
  "author": "Okwukwe Ewurum",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "babel-jest": "^26.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "express": "^4.16.3",
    "fancy-log": "^1.3.3",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.isempty": "^4.4.0",
    "mongoose": "^5.9.12",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.6",
    "prettier": "^2.0.5",
    "redis": "^3.0.2",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^26.0.1",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2"
  }
}
